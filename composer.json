{
    "name": "chadicus/spl-datastructures",
    "description": "A set of standard datastructures.",
    "keywords": ["spl", "datastructures"],
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^5.0",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "psr-4": { "Chadicus\\Spl\\DataStructures\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "ChadicusTest\\Spl\\DataStructures\\": "tests" }
    }
}
